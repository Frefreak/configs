:{
paste :: String -> IO ()
paste s = do
    (v, _, _, p) <- createProcess (shell "xclip -selection clipboard")
                                  { std_in = CreatePipe }
    case v of
      Just h -> hPutStr h s >> hClose h >> waitForProcess p >> return ()
      Nothing -> putStrLn "failed"
:}
